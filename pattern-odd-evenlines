       for(int i = 1 ; i<=10; i++){
           String s = "";
           if(i%2!=0){
               for(int j = 1; 2*j<=11-i ;j++){
                   s =s +"*";
               }
           }else{
               for(int k = 1; 2*k<=i ;k++){
                   s=s+"*";
               }
           }
           System.out.println(s);
       }
